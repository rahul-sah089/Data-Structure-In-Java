package LinkedList2;

public class LinkListAll {
	Node head = null;
	
	public Node getHead() {
		return head;
	}

	public void traverse(Node head) {
		while (head.getLink() != null) {
			System.out.println("-->" + head.getInfo());
			head = head.getLink();
		}
		System.out.println("-->" + head.getInfo());
	}

	public void insertAtBeginning(Node node) {
		node.setLink(head);
		head = node;
	}

	public void insertAtEnd(Node node) {
		
		//traverse till end
		while(head ){
			
		}
	}

	public static void main(String[] args) {

	}
}
